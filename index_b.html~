<script type="text/javascript">
      $(document).ready(function(){
          $("#ther_read").hide();
          $("#more-massage").hide();
           $("#less-massage-link").hide();
          
          $("#therapist-link").click(function(){
    $("#ther_read").toggle(600);
});          

$("#more-massage-link").click(function(){
    $("#more-massage").show(800);
    $("#less-massage-link").show(800);
    $("#more-massage-link").hide();
});  

$("#less-massage-link").click(function(){
    $("#more-massage").hide(800);
    $("#less-massage-link").hide();
    $("#more-massage-link").show(800);
});  
      });

      
 $(function(){
    $(".dropdown").hover(            
            function() {
                $('.dropdown-menu', this).stop( true, true ).fadeIn("fast");
                $(this).toggleClass('open');
                $('b', this).toggleClass("caret caret-up");                
            },
            function() {
                $('.dropdown-menu', this).stop( true, true ).fadeOut("fast");
                $(this).toggleClass('open');
                $('b', this).toggleClass("caret caret-up");                
            });
    }); 
     

    </script>

</body>
</html>