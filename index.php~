 $(function(){
    $(".dropdown").hover(            
            function() {
                $('.dropdown-menu', this).stop( true, true ).fadeIn("fast");
                $(this).toggleClass('open');
                $('b', this).toggleClass("caret caret-up");                
            },
            function() {
                $('.dropdown-menu', this).stop( true, true ).fadeOut("fast");
                $(this).toggleClass('open');
                $('b', this).toggleClass("caret caret-up");                
            });
    }); 

//$("#book_date").datepicker();
$('#book_date').datepicker({
		  format: 'dd/mm/yyyy',
		  minDate :1, 
        startDate : '1d',
        autoclose: true
		});
$('.ui-datepicker').css('z-index', 999999);
     
$(function () {
  $('[]').tooltip()
})

function removeTreat(row) {
	var del_amount = row.closest('td').prev('td').text();
	//alert(row.closest('tr').children('td:first').text());
	treatment_id_array.splice($.inArray(row.closest('tr').children('td:first').text(),treatment_id_array) ,1 );
	console.log('Treatment_array: '+treatment_id_array);
   row.closest('tr').remove();   
   $('#treat_names').val(treatment_id_array);
   t_amount = t_amount - del_amount; 
   treat_count--;
   $('#count').val(treat_count);
   
}

