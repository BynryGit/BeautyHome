$('#treatment').on('change',function() {
	var treatment_id=$('#treatment :selected').val();
	var area= $("#area :selected").text();
	if(treatment_id == ''){$('#treatType').empty().append('whatever');return false;}
	if(area=='Select your area' || area==''){
		alert('Select an area!');return false;	
	}
	

	 $.post('process.php',{treatment_id:treatment_id}, function(response){
					if(response){						
						document.getElementById("treatType").innerHTML=response;						
						}
				});
});


function randString(x){
    var s = "";
    while(s.length<x&&x>0){
        var r = Math.random();
        s+= (r<0.1?Math.floor(r*100):String.fromCharCode(Math.floor(r*26) + (r>0.5?97:65)));
    }
    return s.toUpperCase();
}

    </script>

