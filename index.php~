<?php
		require ('library.php'); 
        $query = mysql_query("SELECT * FROM treatment");
        if($query==FALSE) {
        		die(mysql_error());
        	}        	
        while ($row = mysql_fetch_array($query)) {
            $t_id = $row['treatment_id'];
            $t_name = $row['treatment_name'];
            echo "<option value='$t_id'>$t_name</option>";
           }
?>
</select>
<div class="help-block with-errors"></div>
</div>
