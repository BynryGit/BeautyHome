<script type="text/javascript">
      $(document).ready(function(){
			 
                                 
$("#show-gallery").click(function(){
    $("#nails").show(800);
    $("#less-nails-link").show(800);
    $("#more-nails-link").hide();
});  

$("#less-nails-link").click(function(){
    $("#nails").hide(800);
    $("#less-nails-link").hide();
    $("#more-nails-link").show(800);
});

$("#more-massage-link").click(function(){
    $("#massage").show(800);
    $("#less-massage-link").show(800);
    $("#more-massage-link").hide();
});  

$("#less-massage-link").click(function(){
    $("#massage").hide(800);
    $("#less-massage-link").hide();
    $("#more-massage-link").show(800);
}); 

$("#more-thread-link").click(function(){
    $("#threading").show(800);
    $("#less-thread-link").show(800);
    $("#more-thread-link").hide();
});  

$("#less-thread-link").click(function(){
    $("#threading").hide(800);
    $("#less-thread-link").hide();
    $("#more-thread-link").show(800);
}); 

$("#more-wax-link").click(function(){
    $("#waxing").show(800);
    $("#less-wax-link").show(800);
    $("#more-wax-link").hide();
});  

$("#less-wax-link").click(function(){
    $("#waxing").hide(800);
    $("#less-wax-link").hide();
    $("#more-wax-link").show(800);
}); 

$("#more-face-link").click(function(){
    $("#facials").show(800);
    $("#less-face-link").show(800);
    $("#more-face-link").hide();
});  

$("#less-face-link").click(function(){
    $("#facials").hide(800);
    $("#less-face-link").hide();
    $("#more-face-link").show(800);
});

$("#more-spray-link").click(function(){
    $("#spray").show(800);
    $("#less-spray-link").show(800);
    $("#more-spray-link").hide();
});  

$("#less-spray-link").click(function(){
    $("#spray").hide(800);
    $("#less-spray-link").hide();
    $("#more-spray-link").show(800);
});

});

      
 $(function(){
    $(".dropdown").hover(            
            function() {
                $('.dropdown-menu', this).stop( true, true ).fadeIn("fast");
                $(this).toggleClass('open');
                $('b', this).toggleClass("caret caret-up");                
            },
            function() {
                $('.dropdown-menu', this).stop( true, true ).fadeOut("fast");
                $(this).toggleClass('open');
                $('b', this).toggleClass("caret caret-up");                
            });
    }); 
     

    </script>

</body>
</html>